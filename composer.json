{
    "name": "shiftechafrica/lumen-form-request",
    "description": "Form request is a package for Lumen that lets developer validate form requests like Laravel does. The package also has artisan commands.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "lumen",
        "form-request",
        "validation",
        "middleware",
        "jobs",
        "controllers"
    ],
    "authors": [
        {
            "name": "shiftechafrica",
            "email": "info@shiftechafrica.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "illuminate/support": "~5.1 || ~7.0",
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0 || ^8.5"
    },
    "autoload": {
        "psr-4": {
            "ShiftechAfrica\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShiftechAfrica\\FormRequestServiceProvider"
            ]
        }
    }
}
