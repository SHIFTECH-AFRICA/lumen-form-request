{
    "name": "shiftechafrica/lumen-take-off",
    "description": "Form request is a package for Lumen that lets developer validate form requests like Laravel does. The package also has artisan commands.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "lumen",
        "form-request",
        "validation",
        "middleware",
        "jobs",
        "controllers"
    ],
    "authors": [
        {
            "name": "shiftechafrica",
            "email": "info@shiftechafrica.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "illuminate/support": "^5.4|^6.0|^7.0|^8.0",
        "symfony/var-dumper": "^3.1|^4.1|^4.2|^4.3|^5.0",
        "psy/psysh": "0.8.*|0.9.*|0.10.*",
        "classpreloader/classpreloader": "^3.0|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0 || ^8.5"
    },
    "autoload": {
        "psr-4": {
            "ShiftechAfrica\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShiftechAfrica\\FormRequestServiceProvider"
            ]
        }
    }
}
